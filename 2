#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main(int argc, char **argv)
{
    FILE *plik;
    char linia[255];
    int wiersz = 0;

    plik = fopen(argv[1], "r");

    int i;
    while (fgets(linia, sizeof(linia), plik))
    {
        wiersz++;
        for(i = 2; i<argc; i+=2)
        {
            if(atoi(argv[i]) == wiersz && atoi(argv[i+1]) == strlen(linia)-1)
            {
                printf("%s", linia);
            }
        }
    }

    fclose(plik);
    return 0;
}

