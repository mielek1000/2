#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main()
{
    FILE *plik;
    char linia[255];
    char* tmp;

    char* szukaj = "abc";
    int ilosc = 3;

    int licznik = 0;

    plik = fopen("plik.txt", "r");

    while(fgets(linia, sizeof(linia), plik))
    {
        tmp = linia;
        licznik=0;

        while((tmp = strstr(tmp, szukaj)))
        {
            tmp += strlen(szukaj);
            licznik++;
        }

        if(licznik == ilosc)
        {
            printf("%s", linia);
        }
    }

    fclose(plik);
    getchar();
    return 0;
}

