#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main(int argc, char **argv)
{
    FILE *plik_we;
    FILE *plik_wy;
    char linia[255];

    plik_we=fopen(argv[2],"r");
    plik_wy=fopen(argv[3],"w");

    int i;
    int j;
    int licznik;

    while (fgets(linia, sizeof(linia), plik_we))
    {
        licznik = 0;
        for (i = 0; i < strlen(argv[1]); ++i)
        {
            for (j = 0; j < strlen(linia); ++j)
            {
                if (linia[j] == argv[1][i])
                {
                    licznik = 1;
                }
                if (licznik)
                {
                    break;
                }
            }
        }
        if(licznik)
        {
            fprintf(plik_wy, "%s", linia);
        }
    }

    fclose(plik_we);
    fclose(plik_wy);
    getchar();
    return 0;
}

66666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666666

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main(int argc, char **argv)
{
    FILE *plik_wy;
    char* nazwa_pliku = argv[2];
    char* zakres = argv[1];
    int min = 0;
    int max = 0;

    while(*zakres)
    {
        if(isdigit(*zakres))
        {
            min=strtol(zakres,&zakres,0);
        }
        else
        {
            zakres++;
            max = strtol(zakres,&zakres,0);
        }
    }

    char ciag[255];
    char* wyraz;
    char pom[] = " ";
    int licznik = 0;
    int i;

    printf("Podaj ciag znakow:\n");
    gets(ciag);
    wyraz = strtok(ciag, pom);

    plik_wy = fopen(nazwa_pliku, "w");

    while(wyraz!=NULL)
    {
        for(i = 0; i<strlen(wyraz); i++)
        {
            licznik++;
        }
        if(licznik>9 && licznik<21)
        {
            fprintf(plik_wy, "%s\n", wyraz);
        }
        licznik = 0;
        wyraz = strtok(NULL, pom);
    }

    fclose(plik_wy);
    return 0;
}

